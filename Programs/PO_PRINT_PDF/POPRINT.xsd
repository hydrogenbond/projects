																						<xs:element name="BOX2-ZIP" maxOccurs="unbounded" type="xs:int"/>
																						<xs:element name="BOX2-COUNTRY" maxOccurs="unbounded" type="xs:int"/>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="BLOCK4" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="BLK4-1-T" maxOccurs="unbounded" type="xs:string"/>
