																<xs:element name="MAT-COST-T" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-COST" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-PC-T" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-PC" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-SHIP-T" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-SHIP" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-SHIPVIA-T" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-SHIPVIA" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-DESC-T" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-DESC" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-ARR-T" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-ARR" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-MEMO-T" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-MEMO" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-FREIGHT-TERMS-T" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-FREIGHT-TERMS" maxOccurs="unbounded" type="xs:short"/>
																<xs:element name="MAT-REF-T" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-REF" maxOccurs="unbounded" type="xs:short"/>
																<xs:element name="MAT-SLS-T" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-SLS" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-EXT" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-EXT-QTY" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="POLINE-NOTES" minOccurs="0" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="POLNOTES-HDR" maxOccurs="unbounded" type="xs:string"/>
																			<xs:element name="POL-NOTES" maxOccurs="unbounded" type="xs:string"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="POLINE-POINTERS" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="PNTR-HDR" maxOccurs="unbounded">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="PNTR-HDR-BR" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="PNTR-HDR-ORD" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="PNTR-HDR-CUST" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="PNTR-HDR-QTY" maxOccurs="unbounded" type="xs:string"/>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="POINTER" maxOccurs="unbounded">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="POINTER-WH" maxOccurs="unbounded" type="xs:byte"/>
																						<xs:element name="POINTER-ORD" maxOccurs="unbounded" type="xs:byte"/>
																						<xs:element name="POINTER-SHIP" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="POINTER-RES" maxOccurs="unbounded" type="xs:decimal"/>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="PO-TOTAL" maxOccurs="unbounded" type="xs:string"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
