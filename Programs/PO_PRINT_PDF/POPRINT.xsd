																		<xs:sequence>
																			<xs:element name="BOX1-HEADER" maxOccurs="unbounded" type="xs:string"/>
																			<xs:element name="BOX1-DATA" maxOccurs="unbounded">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="BOX1-NAME-T" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="BOX1-NAME" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="BOX1-ADD1-T" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="BOX1-ADD1" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="BOX1-ADD2" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="BOX1-CITY-T" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="BOX1-CITY" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="BOX1-ZIP-T" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="BOX1-ZIP" maxOccurs="unbounded" type="xs:int"/>
																						<xs:element name="BOX1-COUNTRY" maxOccurs="unbounded" type="xs:int"/>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="BOX2" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="BOX2-HEADER" maxOccurs="unbounded" type="xs:string"/>
																			<xs:element name="BOX2-DATA" maxOccurs="unbounded">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="BOX2-NAME-T" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="BOX2-NAME" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="BOX2-ADD1-T" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="BOX2-ADD1" maxOccurs="unbounded" type="xs:string"/>
																						<xs:element name="BOX2-ADD2" maxOccurs="unbounded" type="xs:string"/>
