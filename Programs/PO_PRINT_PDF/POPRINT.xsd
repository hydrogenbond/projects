																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="BLOCK5" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="BLK5-1-T" maxOccurs="unbounded" type="xs:string"/>
																			<xs:element name="BLK5-1" maxOccurs="unbounded" type="xs:string"/>
																			<xs:element name="BLK5-2-T" maxOccurs="unbounded" type="xs:string"/>
																			<xs:element name="BLK5-2" maxOccurs="unbounded" type="xs:long"/>
																			<xs:element name="BLK5-3-T" maxOccurs="unbounded" type="xs:string"/>
																			<xs:element name="BLK5-3" maxOccurs="unbounded" type="xs:string"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="PONOTES" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="PONOTE" maxOccurs="unbounded" type="xs:string"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="PO-LINE" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="MAT-BOX-HEADER" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="MAT-BOX-H-LINE-T" maxOccurs="unbounded" type="xs:string"/>
																			<xs:element name="MAT-BOX-H-LINE" maxOccurs="unbounded" type="xs:byte"/>
																			<xs:element name="MAT-BOX-H-ORD" maxOccurs="unbounded" type="xs:string"/>
																			<xs:element name="MAT-BOX-H-DATE" minOccurs="0" maxOccurs="unbounded" type="xs:string"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="PO-WHSE" maxOccurs="unbounded" type="xs:byte"/>
																<xs:element name="PO-NUM" maxOccurs="unbounded" type="xs:int"/>
																<xs:element name="PO-LINE" maxOccurs="unbounded" type="xs:byte"/>
																<xs:element name="MAT-STYLE-T" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-STYLE" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-COLOR-T" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-COLOR" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-QTYORD-T" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="MAT-QTYORD" maxOccurs="unbounded" type="xs:string"/>
