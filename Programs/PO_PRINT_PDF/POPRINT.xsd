										<xs:element name="LOGO-LINE1" maxOccurs="unbounded" type="xs:string"/>
										<xs:element name="LOGO-LINE2" maxOccurs="unbounded" type="xs:string"/>
										<xs:element name="LOGO-LINE3" maxOccurs="unbounded" type="xs:string"/>
										<xs:element name="LOGO-LINE4" maxOccurs="unbounded" type="xs:string"/>
										<xs:element name="LOGO-LINE5" maxOccurs="unbounded">
											<xs:complexType/>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="USER-INFO" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="USER" maxOccurs="unbounded" type="xs:string"/>
										<xs:element name="DATE" maxOccurs="unbounded" type="xs:string"/>
										<xs:element name="TIME" maxOccurs="unbounded" type="xs:string"/>
										<xs:element name="TITLE" maxOccurs="unbounded" type="xs:string"/>
										<xs:element name="PO-TITLE" maxOccurs="unbounded" type="xs:string"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="MATERIAL" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="BRANCH" maxOccurs="unbounded">
								<xs:complexType mixed="true">
									<xs:sequence>
										<xs:element name="PO" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="MATERIAL-HEADER" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="MAT-TITLE-HEADER" maxOccurs="unbounded" type="xs:string"/>
																<xs:element name="BLOCK1" maxOccurs="unbounded">
