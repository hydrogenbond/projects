											<xs:element name="BUNDLE-UOM" maxOccurs="unbounded" type="xs:string"/>
											<xs:element name="BUNDLE-PRICE" maxOccurs="unbounded" type="xs:string"/>
											<xs:element name="BUNDLE-AMT" maxOccurs="unbounded" type="xs:string"/>
